# n8n configuration
N8N_HOST=localhost
N8N_PROTOCOL=http
N8N_ENCRYPTION_KEY=replace-with-secure-encryption-key
GENERIC_TIMEZONE=UTC

# Credentials for n8n basic auth
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-this-password

# AWS credentials for n8n workflows
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1

# Database connection for loan eligibility system
LOAN_DB_HOST=your-rds-endpoint
LOAN_DB_NAME=loaneligibility
LOAN_DB_USER=admin
LOAN_DB_PASSWORD=your-db-password

# AI API keys
OPENAI_API_KEY=your-openai-api-key
# or
GEMINI_API_KEY=your-gemini-api-key

# Email settings
SENDER_EMAIL=notifications@yourdomain.com
